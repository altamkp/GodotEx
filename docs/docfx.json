{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": ["**/*.csproj"],
          "exclude": "**/test/**"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appFaviconPath": "images/altamkp.ico",
      "_appFooter": "Copyright (c) 2024 <a href='https://github.com/altamkp'>altamkp</a>",
      "_appLogoPath": "images/altamkp.png",
      "_appName": "GodotEx",
      "_appTitle": "GodotEx",
      "_enableSearch": true,
      "_googleAnalyticsTagId": "G-44V922BNFV",
      "pdf": true
    }
  }
}
